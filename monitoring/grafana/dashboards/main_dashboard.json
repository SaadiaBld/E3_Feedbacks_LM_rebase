{
  "__inputs": [],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "7.5.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "mappings": [], "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null } ] } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "id": 1,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(last_over_time(verbatims_analyzed_total{job=\"verbatim_pipeline\"}[$__range])) or sum(last_over_time(verbatims_analyzed_total{exported_job=\"verbatim_pipeline\"}[$__range]))",
          "legendFormat": "Analyzed (snapshot)",
          "refId": "A"
        }
      ],
      "title": "Verbatims Analyzed (snapshot)",
      "type": "stat"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "mappings": [], "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 1 } ] } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(last_over_time(errors_json_total{job=\"verbatim_pipeline\"}[$__range])) or sum(last_over_time(errors_json_total{exported_job=\"verbatim_pipeline\"}[$__range]))",
          "legendFormat": "JSON Errors (snapshot)",
          "refId": "A"
        }
      ],
      "title": "JSON Errors (snapshot)",
      "type": "stat"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": {
        "defaults": { "color": { "mode": "thresholds" }, "mappings": [], "thresholds": { "mode": "absolute", "steps": [ { "color": "green", "value": null }, { "color": "red", "value": 1 } ] } },
        "overrides": []
      },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(last_over_time(bq_insert_errors_total{job=\"verbatim_pipeline\"}[$__range])) or sum(last_over_time(bq_insert_errors_total{exported_job=\"verbatim_pipeline\"}[$__range]))",
          "legendFormat": "BQ Insert Errors (snapshot)",
          "refId": "A"
        }
      ],
      "title": "BigQuery Insert Errors (snapshot)",
      "type": "stat"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 8 },
      "id": 4,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "expr": "(\n  sum(last_over_time(verbatim_analysis_duration_seconds_sum{job=\"verbatim_pipeline\"}[$__range]))\n  /\n  sum(last_over_time(verbatim_analysis_duration_seconds_count{job=\"verbatim_pipeline\"}[$__range]))\n)\nOR\n(\n  sum(last_over_time(verbatim_analysis_duration_seconds_sum{exported_job=\"verbatim_pipeline\"}[$__range]))\n  /\n  sum(last_over_time(verbatim_analysis_duration_seconds_count{exported_job=\"verbatim_pipeline\"}[$__range]))\n)",
          "legendFormat": "Avg Analysis Duration (snapshot)",
          "refId": "A"
        }
      ],
      "title": "Average Analysis Duration (snapshot)",
      "type": "timeseries"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "unit": "percent" }, "overrides": [] },
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 8 },
      "id": 5,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" }, "tooltip": { "mode": "single" } },
      "targets": [
        {
          "expr": "100 * (\n  sum(last_over_time(claude_calls_total{job=\"verbatim_pipeline\",status=\"success\"}[$__range]))\n  /\n  sum(last_over_time(claude_calls_total{job=\"verbatim_pipeline\"}[$__range]))\n)\nOR\n100 * (\n  sum(last_over_time(claude_calls_total{exported_job=\"verbatim_pipeline\",status=\"success\"}[$__range]))\n  /\n  sum(last_over_time(claude_calls_total{exported_job=\"verbatim_pipeline\"}[$__range]))\n)",
          "legendFormat": "Claude Success Rate (snapshot)",
          "refId": "A"
        }
      ],
      "title": "Claude Success Rate (snapshot)",
      "type": "timeseries"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 18 },
      "id": 6,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(last_over_time(claude_response_empty_total{job=\"verbatim_pipeline\"}[$__range])) or sum(last_over_time(claude_response_empty_total{exported_job=\"verbatim_pipeline\"}[$__range]))",
          "legendFormat": "Empty Responses (snapshot)",
          "refId": "A"
        }
      ],
      "title": "Claude Empty Responses (snapshot)",
      "type": "stat"
    },

    {
      "datasource": "Prometheus",
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" } }, "overrides": [] },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 18 },
      "id": 7,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [
        {
          "expr": "sum(last_over_time(verbatims_skipped_total{job=\"verbatim_pipeline\"}[$__range])) or sum(last_over_time(verbatims_skipped_total{exported_job=\"verbatim_pipeline\"}[$__range]))",
          "legendFormat": "Skipped (snapshot)",
          "refId": "A"
        }
      ],
      "title": "Verbatims Skipped (snapshot)",
      "type": "stat"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 27,
  "style": "dark",
  "tags": [],
  "templating": { "list": [] },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Application Monitoring (Snapshot)",
  "uid": "verbatim-snapshot",
  "version": 3
}
